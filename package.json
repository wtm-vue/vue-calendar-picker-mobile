{
  "name": "vue-calendar-picker-mobile",
  "version": "1.1.11",
  "author": "tianmei.wu",
  "description": "a date picker component form vue mobile H5",
  "main": "./lib/index.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lib": "vue-cli-service build  --mode production --target lib --name index --dest lib packages/index.js ",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "moment": "2.24.0",
    "vue": "^2.6.10"
  },
  "peerDependencies": {
    "moment": "^2.24.0",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [
    "vue",
    "mobile",
    "h5",
    "date-picker",
    "datepicker",
    "date-range",
    "daterange",
    "month-range",
    "monthrange",
    "year-range",
    "yearrange",
    "calendar",
    "calendarpicker"
  ],
  "license": "ISC",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wtm-vue/vue-calendar-picker-mobile.git"
  },
  "bugs": {
    "url": "https://github.com/wtm-vue/vue-calendar-picker-mobile/issues"
  },
  "homepage": "https://github.com/wtm-vue/vue-calendar-picker-mobile#readme"
}
